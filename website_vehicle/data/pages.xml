<odoo>
	<data>
		<record id="hello_page" model="website.page">
			<field name="name">Hello World</field>
			<field name="is_published">True</field>
			<field name="url">/module/hello</field>
			<field name="track">False</field>
			<field name="type">qweb</field>
			<field name="key">website_vehicle.hello_page</field>
			<field name="is_homepage">False</field>
			<field name="arch" type="xml">
				<t t-call="website.layout">
					<div id="wrap" class="oe_structure home-page">
						<h1 class="text-primary text-center">Hello World from the module</h1>
					</div>
				</t>
			</field>
		</record>
	</data>
</odoo>